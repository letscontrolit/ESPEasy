{
    "build": {
      "arduino":{
        "ldscript": "esp32c2_out.ld"
      },
      "core": "esp32",
      "extra_flags": "-DARDUINO_TASMOTA -DESP32_2M -DESP32C2",
      "f_cpu": "120000000L",
      "f_flash": "60000000L",
      "flash_mode": "qio",
      "mcu": "esp32c2",
      "variant": "esp32c2",
      "partitions": "boards/partitions/esp32_partition_app1700k_spiffs320k_noOTA.csv"
    },
    "connectivity": [
      "wifi",
      "bluetooth"
    ],
    "debug": {
      "openocd_target": "esp32c2.cfg"
    },
    "frameworks": [
      "arduino",
      "espidf"
    ],
    "name": "Espressif Generic ESP32-C2 = 2M Flash, ESPEasy 1810k Code/No OTA 316k FS",
    "upload": {
      "flash_size": "2MB",
      "maximum_ram_size": 278528,
      "maximum_size": 2097152,
      "require_upload_port": true,
      "speed": 460800
    },
    "url": "https://docs.espressif.com/projects/espressif-esp-dev-kits/en/latest/esp32c6/esp32-c6-devkitc-1/index.html",
    "vendor": "Espressif"
